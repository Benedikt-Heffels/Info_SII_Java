<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="_04_Tic_Tac_Toe.TTT_Controller">
    <children>
        <GridPane fx:id="SolutionGrid" gridLinesVisible="true" layoutX="123.0" layoutY="194.0" prefHeight="206.0" prefWidth="355.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <TextField prefHeight="61.0" prefWidth="119.0" text="1.x" GridPane.columnIndex="1" />
                <TextField prefHeight="69.0" prefWidth="118.0" text="2.x" GridPane.columnIndex="2" />
                <TextField prefHeight="57.0" prefWidth="118.0" text="3.x" GridPane.columnIndex="3" />
                <TextField prefHeight="61.0" prefWidth="88.0" text="x.1" GridPane.rowIndex="1" />
                <TextField prefHeight="65.0" prefWidth="88.0" text="x.2" GridPane.rowIndex="2" />
                <TextField prefHeight="73.0" prefWidth="88.0" text="x.3" GridPane.rowIndex="3" />
            </children>
        </GridPane>
        <TextField layoutX="123.0" layoutY="32.0" prefHeight="25.0" prefWidth="355.0" text="Spieler, bitte gebe das Feld, wo du dein Kreuz setzen will, ein." />
        <TextField fx:id="tfActivePlayer" layoutX="123.0" layoutY="7.0" onAction="#tfActivePlayer_printActivePlayer" prefHeight="25.0" prefWidth="355.0" />
        <Button fx:id="btEingabe" layoutX="389.0" layoutY="142.0" mnemonicParsing="false" onAction="#btEingabe_onClick" onMouseClicked="#btEingabe_OnClick()" prefHeight="52.0" prefWidth="89.0" text="Eingabe" />
    </children>
</AnchorPane>
